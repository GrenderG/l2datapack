            }

            if (HaveMemo(talker, @audience_with_earth_dragon) == 1 && (GetMemoState(talker, @audience_with_earth_dragon) == 21110 || GetMemoState(talker, @audience_with_earth_dragon) == 21101 || GetMemoState(talker, @audience_with_earth_dragon) == 21100 || GetMemoState(talker, @audience_with_earth_dragon) == 20111 || GetMemoState(talker, @audience_with_earth_dragon) == 20110 || GetMemoState(talker, @audience_with_earth_dragon) == 20101 || GetMemoState(talker, @audience_with_earth_dragon) == 20100))
            {
                _choiceN = _choiceN + 1;
                _code = 2;
