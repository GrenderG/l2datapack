    EventHandler TALK_SELECTED(fhtml0, talker)
    {
        if (_from_choice == 0)
        {
            if (HaveMemo(talker, @audience_with_earth_dragon) == 1 && (GetMemoState(talker, @audience_with_earth_dragon) == 21011 || GetMemoState(talker, @audience_with_earth_dragon) == 21010 || GetMemoState(talker, @audience_with_earth_dragon) == 21001 || GetMemoState(talker, @audience_with_earth_dragon) == 21000 || GetMemoState(talker, @audience_with_earth_dragon) == 20011 || GetMemoState(talker, @audience_with_earth_dragon) == 20010 || GetMemoState(talker, @audience_with_earth_dragon) == 20001 || GetMemoState(talker, @audience_with_earth_dragon) == 20000) && OwnItemCount(talker, @q_remains_of_sacrificed) == 0)
            {
                _choiceN = _choiceN + 1;
                _code = 0;
                AddChoice(0, "Ask about conquering Antharas");
            }

            if (HaveMemo(talker, @audience_with_earth_dragon) == 1 && (GetMemoState(talker, @audience_with_earth_dragon) == 21011 || GetMemoState(talker, @audience_with_earth_dragon) == 21010 || GetMemoState(talker, @audience_with_earth_dragon) == 21001 || GetMemoState(talker, @audience_with_earth_dragon) == 21000 || GetMemoState(talker, @audience_with_earth_dragon) == 20011 || GetMemoState(talker, @audience_with_earth_dragon) == 20010 || GetMemoState(talker, @audience_with_earth_dragon) == 20001 || GetMemoState(talker, @audience_with_earth_dragon) == 20000) && OwnItemCount(talker, @q_remains_of_sacrificed) == 1)
            {
                _choiceN = _choiceN + 1;
                _code = 1;
