class gatekeeper_pontina : instant_teleporter
{
handler:
    EventHandler TELEPORT_REQUESTED(talker)
    {
        if (Rand(100) < 50)
        {
            InstantTeleport(talker, 17252, 114121, -3439);
        }
        else
        {
            InstantTeleport(talker, 17253, 114232, -3439);
        }
    }
}
